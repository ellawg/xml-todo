<data>
    <todos>
        {{#each todos}}
        <todo id="{{id}}" redirect="{{../redirect}}">
            <task>{{task}}</task>
            <note>{{note}}</note>
            <categoryId>{{category}}</categoryId>
            <due>{{due}}</due>
            <created>{{created}}</created>
            <completed>{{completed}}</completed>
        </todo>
        {{/each}}
    </todos>
    <completed-todos>
        {{#each completedTodos}}
        <todo id="{{id}}" redirect="{{../redirect}}">
            <task>{{task}}</task>
            <note>{{note}}</note>
            <categoryId>{{category}}</categoryId>
            <due>{{due}}</due>
            <created>{{created}}</created>
            <completed>{{completed}}</completed>
        </todo>
        {{/each}}
    </completed-todos>
    <categories>
        {{#each categories}}
        <category id="{{id}}">{{category}}</category>
        {{/each}}
    </categories>
</data>